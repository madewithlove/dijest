    "email": "heroes@madewithlove.be"
  },
  "dependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "immutable": "^3.8.1",
    "react-test-renderer": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "jest": "^20.0.4",
